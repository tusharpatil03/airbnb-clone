<% if(success[0]){ %>
    <div class="alert alert-primary alert-dismissible fade show col-6 offset-2" role="alert">
        <%= success %>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
    </div>
    <% } %>

        <% if(error[0]){ %>
            <div class="alert alert-danger alert-dismissible fade show col-6 offset-2" role="alert">
                <%= error %>
                    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            </div>
            <% } %>